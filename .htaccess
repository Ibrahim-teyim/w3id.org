Options +FollowSymLinks
RewriteEngine on

# Redirect for TV Shows
RewriteRule ^mykg/tvShow/(.*)$ https://Ibrahim-teyim.github.io/mykg/tvShow/$1 [R=302,L]

# Redirect for Actors (profit)
RewriteRule ^mykg/actor/(.*)$ https://Ibrahim-teyim.github.io/mykg/actor/$1 [R=302,L]

# Redirect for Actors (ratings)
RewriteRule ^mykg/actorRating/(.*)$ https://Ibrahim-teyim.github.io/mykg/actorRating/$1 [R=302,L]

# Redirect for Music Popularity
RewriteRule ^mykg/artist/(.*)$ https://Ibrahim-teyim.github.io/mykg/artist/$1 [R=302,L]

# Redirect for News
RewriteRule ^mykg/news/(.*)$ https://Ibrahim-teyim.github.io/mykg/news/$1 [R=302,L]

# Redirect for Graphs
RewriteRule ^mykg/graph/(.*)$ https://Ibrahim-teyim.github.io/mykg/graph/$1 [R=302,L]
